    "concurrently": "^7.6.0",
    "electron": "^23.1.4",
    "electron-builder": "^23.6.0",
    "electron-vite": "^1.0.20",
    "eslint": "^8.5.0",
    "eslint-plugin-vue": "^9.2.0",
    "glob": "^7.2.0",
    "http-server": "^14.1.0",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
