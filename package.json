    "core-js": "^3.19.1",
    "three": "^0.127.0",
    "vue": "^2.6.14",
    "vue-chartjs": "^3.5.1",
    "vue-gl": "^1.0.0",
    "vue-moment": "^4.1.0",
    "vue-router": "^3.5.3",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.5.15",
    "@vue/cli-plugin-eslint": "^4.5.15",
    "@vue/cli-service": "^4.5.15",
    "babel-eslint": "^10.1.0",
    "electron": "^16.0.1",
    "electron-devtools-installer": "^3.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
