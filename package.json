{
  "name": "quic-config",
  "version": "0.5.7",
  "private": true,
  "scripts": {
    "serve": "vite",
    "build": "npm run build:clean && npm run build:vue && npm run build:nw",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "build:clean": "rimraf ./dist ./build",
    "build:vue": "vue-tsc --noEmit && vite build",
    "build:gh-pages": "env DEPLOYMENT=gh-pages vue-tsc --noEmit && env DEPLOYMENT=gh-pages vite build --outDir docs",
    "build:linux": "npm run build:linux:clean && npm run build:vue && build --tasks linux-x64 --mirror https://dl.nwjs.io/ .",
    "build:linux:clean": "rimraf ./dist ./build/quic-config*linux*",
    "build:mac": "npm run build:mac:clean && npm run build:vue && build --tasks mac-x64 --mirror https://dl.nwjs.io/ .",
    "build:mac:clean": "rimraf ./dist ./build/quic-config*mac*",
    "build:nw": "build --concurrent --tasks win-x64,linux-x64,mac-x64 --mirror https://dl.nwjs.io/ .",
    "build:windows": "npm run build:windows:clean && npm run build:vue && build --tasks win-x64 --mirror https://dl.nwjs.io/ .",
    "build:windows:clean": "rimraf ./dist ./build/quic-config*win*",
    "pwa": "env DEPLOYMENT=local vue-tsc --noEmit && env DEPLOYMENT=local vite build && npx http-server ./dist",
    "start": "concurrently \"npm run serve\" \"nw .\""
  },
  "main": "http://localhost:8080",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.1.1",
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-brands-svg-icons": "^6.1.1",
    "@fortawesome/free-regular-svg-icons": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/vue-fontawesome": "^3.0.1",
    "@octokit/rest": "^19.0.3",
    "@vue/compat": "^3.1.0",
    "bulma": "^0.9.4",
    "bulma-switch": "^2.0.4",
    "chart.js": "^3.8.0",
    "chartjs-adapter-moment": "^1.0.0",
    "extract-zip": "^2.0.1",
    "follow-redirects": "^1.14.9",
    "isomorphic-fetch": "^3.0.0",
    "md5": "^2.3.0",
    "moment": "^2.29.3",
    "pinia": "^2.0.15",
    "semver": "^7.3.5",
    "three": "^0.142.0",
    "ts-enum-util": "^4.0.2",
    "util": "^0.12.4",
    "vue": "^3.1.0",
    "vue-chartjs": "^4.1.1",
    "vue-router": "^4.0.0",
    "vuex": "^4.0.2",
    "web-serial-polyfill": "^1.0.13",
    "yaml": "^2.1.1"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.0",
    "@types/dom-serial": "^1.0.2",
    "@types/node": "^16.11.27",
    "@types/nw.js": "^0.13.14",
    "@types/semver": "^7.3.9",
    "@types/w3c-web-usb": "^1.0.6",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/tsconfig": "^0.1.3",
    "concurrently": "^6.5.1",
    "eslint": "^8.5.0",
    "eslint-plugin-vue": "^9.2.0",
    "glob": "^7.2.0",
    "http-server": "^14.1.0",
    "nw": "0.54.0-sdk",
    "nwjs-builder-phoenix": "^1.15.0",
    "nwjs-types": "^1.0.0",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "sass": "^1.52.1",
    "typescript": "~4.6.3",
    "vite": "^2.9.5",
    "vite-plugin-pwa": "^0.12.3",
    "vue-tsc": "^0.38.3"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "build": {
    "nwVersion": "v0.54.0",
    "nwFlavor": "normal",
    "output": "./build",
    "targets": [
      "zip",
      "nsis"
    ],
    "files": [
      "dist/**/*",
      "public/**/*"
    ],
    "strippedProperties": [
      "ManifestComments",
      "scripts",
      "devDependencies",
      "build"
    ],
    "overriddenProperties": {
      "main": "dist/index.html"
    },
    "win": {
      "icon": "public/favicon.ico"
    },
    "mac": {
      "icon": "public/logo.icns"
    },
    "nsis": {
      "icon": "public/favicon.ico",
      "unIcon": "public/favicon.ico",
      "languages": [
        "English"
      ],
      "diffUpdaters": false,
      "hashCalculation": true
    }
  },
  "node-remote": [
    "http://localhost:8080/*",
    "file:///dist/*"
  ],
  "window": {
    "width": 1280,
    "height": 720,
    "min_width": 960,
    "min_height": 600,
    "icon": "public/Logo.png"
  }
}
